<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.assignment.truestrength"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="24" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.assignment.truestrength.SplashActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.assignment.truestrength.MainActivity" >
            <intent-filter>
                <action android:name="com.assignment.truestrength.MAINACIVITY" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="com.assignment.truestrength.Programme" />
        <activity android:name="com.assignment.truestrength.Progress" />
        <activity android:name="com.assignment.truestrength.Search" />
        <activity android:name="com.assignment.truestrength.Login" />
    </application>

</manifest>