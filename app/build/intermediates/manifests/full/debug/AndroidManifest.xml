<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.assignment.truestrength"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="24" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.assignment.truestrength.SplashActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.assignment.truestrength.MainActivity" >
            <intent-filter>
                <action android:name="com.assignment.truestrength.MAINACIVITY" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.assignment.truestrength.Programme"
            android:label="Exercise Program"
            android:parentActivityName="com.assignment.truestrength.MainActivity" >
        </activity>
        <activity
            android:name="com.assignment.truestrength.Progress"
            android:label="Progress"
            android:parentActivityName="com.assignment.truestrength.MainActivity" >
        </activity>
        <activity
            android:name="com.assignment.truestrength.Search"
            android:label="Search"
            android:parentActivityName="com.assignment.truestrength.MainActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity android:name="com.assignment.truestrength.Login" />
        <!--
 ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>